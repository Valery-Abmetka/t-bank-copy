{
  "name": "t-bank-copy",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev:app": "concurrently \"npm run dev:investments\" \"npm run dev:host\"",
    "dev:host": "npm run dev -w host",
    "dev:investments": "npm run dev -w investments",
    "build:investments": "npm run build:prod -w investments",
    "build:host": "npm run build:prod -w host",
    "build:app": "concurrently \"npm run build:investments\" \"npm run build:host\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.8.0"
  }
}
